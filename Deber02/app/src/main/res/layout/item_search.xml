<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="10dp"
        android:background="@color/white1"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/btn_item_buscar"
            android:layout_width="180dp"
            android:layout_height="90dp"
            android:background="@drawable/regueton"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/btn_item_buscar2"
            android:layout_width="184dp"
            android:layout_height="94dp"
            android:background="@drawable/madona"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/btn_item_buscar3"
            android:layout_width="180dp"
            android:layout_height="86dp"
            android:background="@drawable/ecuador"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/btn_item_buscar4"
            android:layout_width="180dp"
            android:layout_height="90dp"
            android:background="@drawable/mix"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/btn_item_buscar5"
            android:layout_width="184dp"
            android:layout_height="94dp"
            android:background="@drawable/cancion2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/btn_item_buscar6"
            android:layout_width="184dp"
            android:layout_height="94dp"
            android:background="@drawable/sinbandera"
            tools:ignore="SpeakableTextPresentCheck" />
    </LinearLayout>


</LinearLayout>